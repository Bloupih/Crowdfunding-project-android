<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supinfo.supcrowdfundingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.IndexActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.ListCategoryActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.ListProjectsCategoryActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.ShowProjectActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.SeConnecterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.AccountActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.AddProjectActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.activity.EditProfileActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.supinfo.supcrowdfundingapp.TestActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
